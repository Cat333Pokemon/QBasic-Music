SUB BIGLINE (ENDTYPE AS INTEGER)
COLOR 7
PRINT "             ";
IF ENDTYPE = 1 THEN
        PRINT "É";
ELSEIF ENDTYPE = 2 THEN
        PRINT "Ì";
ELSEIF ENDTYPE = 3 THEN
        PRINT "Ç";
ELSEIF ENDTYPE = 4 THEN
        PRINT "È";
END IF
IF ENDTYPE = 3 THEN
        PRINT "ÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ";
ELSE
        PRINT "ÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍ";
END IF
IF ENDTYPE = 1 THEN
        PRINT "»"
ELSEIF ENDTYPE = 2 THEN
        PRINT "¹"
ELSEIF ENDTYPE = 3 THEN
        PRINT "¶"
ELSEIF ENDTYPE = 4 THEN
        PRINT "¼"
END IF
END SUB

SUB LYRIC (VAR AS STRING, VARCOLOR AS INTEGER)
DIM i AS INTEGER
DIM timeC AS DOUBLE
i = LEN(VAR)

COLOR 7
PRINT "             º ";
COLOR VARCOLOR
'PRINT VAR;
FOR k = 1 TO LEN(VAR)
    PRINT MID$(VAR, k, 1);
    timeC = TIMER
    DO
    LOOP UNTIL TIMER <> timeC
NEXT k
FOR j = 0 TO (50 - i)
    PRINT " ";
NEXT j
COLOR 7
PRINT " º"

IF LEN(VAR) > 10 THEN
    DO UNTIL PLAY(0) = 0
        'Delay
    LOOP
END IF

END SUB

